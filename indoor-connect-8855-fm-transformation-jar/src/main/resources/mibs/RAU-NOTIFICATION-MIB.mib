		-- 1.3.6.1.4.1.193.235.1.1.2.1.1.4
		alarmAlerts OBJECT IDENTIFIER ::= { rauNotificationObjs 4 }

		
		-- 1.3.6.1.4.1.193.235.1.1.2.1.1.4.1
		alarmAlertNumber OBJECT-TYPE
			SYNTAX Integer32
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"The number of rows in the alert table."
			::= { alarmAlerts 1 }

		
		-- 1.3.6.1.4.1.193.235.1.1.2.1.1.4.2
		alarmAlertLastChanged OBJECT-TYPE
			SYNTAX DateAndTime
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"A timestamp when the alert table was last
				                changed. Can be used by a manager to initiate 
				                an update of alerts procedure.
				                NOTE: All fields of the DateAndTime MUST be
				                filled out, including the hours and minutes from
				                UTC. As such, the value should be 11 octets
				                long."
			::= { alarmAlerts 2 }

		
		-- 1.3.6.1.4.1.193.235.1.1.2.1.1.4.3
		alarmAlertLastSequenceNo OBJECT-TYPE
			SYNTAX Integer32
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"The last sequence number used in alert
				                notifications. It can be used to detect a lost
				                notification."
			::= { alarmAlerts 3 }

		
		-- 1.3.6.1.4.1.193.235.1.1.2.1.1.4.5
		alarmAlertTable OBJECT-TYPE
			SYNTAX SEQUENCE OF AlarmAlertEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"This table lists the last alerts in the system.
				                An alert is a stateless event, that is no clear
				                notification will be sent. Rows in this table are
				                created when a new alert event is detected. Rows
				                are deleted by the instrumentation at a
				                predefined size or age."
			::= { alarmAlerts 5 }

		
		-- 1.3.6.1.4.1.193.235.1.1.2.1.1.4.5.1
		alarmAlertEntry OBJECT-TYPE
			SYNTAX AlarmAlertEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"One entry in the table holds one alert for a
				                given resource. Rows in this table are created
				                when a new alert is detected. Rows are deleted
				                by the instrumentation at a predefined size or
				                age."
			INDEX { alarmAlertIndex }
			::= { alarmAlertTable 1 }

		
		AlarmAlertEntry ::=
			SEQUENCE { 
				alarmAlertIndex
					Unsigned32,
				alarmAlertMajorType
					OCTET STRING,
				alarmAlertMinorType
					OCTET STRING,
				alarmAlertSpecificProblem
					OCTET STRING,
				alarmAlertManagedObject
					OCTET STRING,
				alarmAlertEventType
					OCTET STRING,
				alarmAlertEventTime
					DateAndTime,
				alarmAlertProbableCause
					OCTET STRING,
				alarmAlertSeverity
					OCTET STRING,
				alarmAlertAdditionalText
					OCTET STRING,
				alarmAlertResourceId
					OBJECT IDENTIFIER,
				alarmAlertAdditionalInfo
					OCTET STRING
			 }

		-- 1.3.6.1.4.1.193.235.1.1.2.1.1.4.5.1.1
		alarmAlertIndex OBJECT-TYPE
			SYNTAX Unsigned32 (1..65535)
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Index in the alert table"
			::= { alarmAlertEntry 1 }

		
		-- 1.3.6.1.4.1.193.235.1.1.2.1.1.4.5.1.2
		alarmAlertMajorType OBJECT-TYPE
			SYNTAX OCTET STRING
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"In combination with alarmAlertMinorType, this object
				                provides unique identification of the alert.
						This is a simplification of the different X.733
				                and 3GPP alarm IRP alarm correlation mechanisms
				                based on EventType, ProbableCause,
				                SpecificProblem and NotificationId."
			::= { alarmAlertEntry 2 }

		
		-- 1.3.6.1.4.1.193.235.1.1.2.1.1.4.5.1.3
		alarmAlertMinorType OBJECT-TYPE
			SYNTAX OCTET STRING
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"In combination with alarmAlertMajorType, this object
				                provides a unique identification of the alert.
						This is a simplification of the different X.733
				                and 3GPP alarm IRP alarm correlation mechanisms
				                based on EventType, ProbableCause,
				                SpecificProblem and NotificationId."
			::= { alarmAlertEntry 3 }

		
		-- 1.3.6.1.4.1.193.235.1.1.2.1.1.4.5.1.4
		alarmAlertSpecificProblem OBJECT-TYPE
			SYNTAX OCTET STRING
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"This is a clear-text unique identification of
				                the alert type. There is a one-to-one mapping
				                between alarmAlertSpecificProblem and
				                [alarmAlertMajorType, alarmAlertMinorType]"
			::= { alarmAlertEntry 4 }

		
		-- 1.3.6.1.4.1.193.235.1.1.2.1.1.4.5.1.5
		alarmAlertManagedObject OBJECT-TYPE
			SYNTAX OCTET STRING
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"The 3GPP naming convention shall be used as
				                format for the managed object parameter. Note
				                that the granularity must be good enough to
				                guarantee relevant resource identification to the
				                operator.
				                NOTE: The DN should be *relative* to the nodes
				                *own* root."
			REFERENCE
				"3GPP TS 32.106-8 V3.2, Name convention for
				                Managed Objects"
			::= { alarmAlertEntry 5 }

		
		-- 1.3.6.1.4.1.193.235.1.1.2.1.1.4.5.1.6
		alarmAlertEventType OBJECT-TYPE
			SYNTAX OCTET STRING
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"The event type as defined in X.733/X.736."
			REFERENCE
				"ITU Recommendation X.733, 'Information
				                Technology - Open Systems Interconnection -
				                System Management: Alarm Reporting Function',
				                1992"
			::= { alarmAlertEntry 6 }

		
		-- 1.3.6.1.4.1.193.235.1.1.2.1.1.4.5.1.7
		alarmAlertEventTime OBJECT-TYPE
			SYNTAX DateAndTime
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"A timestamp of the alert. NOTE: All fields of
				                the DateAndTime MUST be filled out, including the
				                hours and minutes from UTC. As such, the value
				                should be 11 octets long."
			REFERENCE
				"DateAndTime is defined in RFC 2579, page 18.
				                ITU Recommendation X.733, 'Information Technology
				                - Open Systems Interconnection - System
				                Management: Alarm Reporting Function', 1992"
			::= { alarmAlertEntry 7 }

		
		-- 1.3.6.1.4.1.193.235.1.1.2.1.1.4.5.1.8
		alarmAlertProbableCause OBJECT-TYPE
			SYNTAX OCTET STRING
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"The probable cause, originally defined by X.733 
				                and subsequent standards. Due to the
				                history of problems in maintaining a standardized
				                probable cause the probable cause is not unique.
				                A best effort mapping of the alert to existing
				                probable causes is used."
			REFERENCE
				"ITU Recommendation X.733, 'Information
				                Technology - Open Systems Interconnection -
				                System Management: Alarm Reporting Function',
				                1992"
			::= { alarmAlertEntry 8 }

		
		-- 1.3.6.1.4.1.193.235.1.1.2.1.1.4.5.1.9
		alarmAlertSeverity OBJECT-TYPE
			SYNTAX OCTET STRING
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"The severity of the alert as defined by X.733"
			REFERENCE
				"ITU Recommendation X.733, 'Information
				                Technology - Open Systems Interconnection -
				                System Management: Alarm Reporting Function',
				                1992"
			::= { alarmAlertEntry 9 }

		
		-- 1.3.6.1.4.1.193.235.1.1.2.1.1.4.5.1.10
		alarmAlertAdditionalText OBJECT-TYPE
			SYNTAX OCTET STRING
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"A user friendly text describing the alert. The
				                text is both static depending on the alert type
				                and dynamic depending on managed object and other
				                conditions."
			REFERENCE
				"ITU Recommendation X.733, 'Information
				                Technology - Open Systems Interconnection -
				                System Management: Alarm Reporting Function',
				                1992"
			::= { alarmAlertEntry 10 }

		
		-- 1.3.6.1.4.1.193.235.1.1.2.1.1.4.5.1.11
		alarmAlertResourceId OBJECT-TYPE
			SYNTAX OBJECT IDENTIFIER
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"In case the alert refers to an object which is
				                instrumented by SNMP, this variable indicates the
				                corresponding OID for the managed object."
			REFERENCE
				"RFC 3877 IETF ALARM MIB"
			::= { alarmAlertEntry 11 }

		
		-- 1.3.6.1.4.1.193.235.1.1.2.1.1.4.5.1.12
		alarmAlertAdditionalInfo OBJECT-TYPE
			SYNTAX OCTET STRING
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Additional information for the alert,
								structured in a way that is suitable for
								machine to machine communication.
				                This string is longer than the corresponding
				                varbind in the notification in order to manage
				                large strings."
			::= { alarmAlertEntry 12 }

		
		-- 1.3.6.1.4.1.193.235.1.1.2.1.1.5
		alarmHeartBeat OBJECT IDENTIFIER ::= { rauNotificationObjs 5 }

		
		-- 1.3.6.1.4.1.193.235.1.1.2.1.1.5.1
		alarmHbInterval OBJECT-TYPE
			SYNTAX Integer32 (0..86399)
			UNITS "s"
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"The notification alarmHeartBeatNotif will be
				                sent by every alarmHbInterval. Managers can
				                subscribe to the notification using the SNMP
				                framework MIBS by using the snmpNotifyName
				                'heartbeat'. 0 means no need send trap to NMS."
			DEFVAL { 180 }
			::= { alarmHeartBeat 1 }

		
		-- 1.3.6.1.4.1.193.235.1.1.2.1.1.5.2
		alarmHeartbeatString OBJECT-TYPE
			SYNTAX SnmpAdminString
			MAX-ACCESS accessible-for-notify
			STATUS current
			DESCRIPTION
				"Alarm heartbeat string, it will be sent to NMS."
			DEFVAL { "Heartbeat" }
			::= { alarmHeartBeat 2 }

		
		-- 1.3.6.1.4.1.193.235.1.1.2.1.1.5.3
		noNmsContactTimeout OBJECT-TYPE
			SYNTAX Integer32 (0..86399)
			UNITS "s"
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"NMS will get the alarm information periodically. This parameter 
				    is used to set the interval.
				 A value of 0 will suppress the no OAM/NMS contact alarm."
			DEFVAL { 1200 }
			::= { alarmHeartBeat 3 }

		
		-- 1.3.6.1.4.1.193.235.1.1.2.1.2
		alarmNotifications OBJECT IDENTIFIER ::= { notificationMIBModule 2 }

		
		-- 1.3.6.1.4.1.193.235.1.1.2.1.2.1
		rauNotifPrefix OBJECT IDENTIFIER ::= { alarmNotifications 1 }

		
		-- 1.3.6.1.4.1.193.235.1.1.2.1.2.1.2
		alarmWarning NOTIFICATION-TYPE
			OBJECTS { alarmActiveManagedObject, alarmActiveMajorType, alarmActiveMinorType, alarmActiveSpecificProblem, alarmActiveLastSequenceNo, 
				alarmActiveEventType, alarmActiveEventTime, alarmActiveProbableCause, alarmActiveAdditionalText, alarmActiveAdditionalInfo, 
				alarmNObjRecordType }
			STATUS current
			DESCRIPTION 
				"This notification is sent when a resource
				                detects a new alarm state with severity warning.
				                The notification is also used to change severity,
				                additional text and/or additional info
				                of an alarm. The alarmNObjRecordType
				                varbind indicates whether this is a new alarm
				                instance or an update to an existing alarm instance.
				                
				                The combination of ManagedObject and 
				                MajorType/MinorType is always unique and can be
				                used by management systems to correlate alarms,
				                change alarms, and clear alarms. A corresponding
				                row will be created in the alarm table
				                (alarmActiveTable). The sequence number
				                will increase for every notification and can be
				                used to detect lost notifications.
				
				                A management system should be prepared for
				                appending text to additional text, indicated by
				                the alarmNObjMoreAdditionalText varbind, and
				                sent with alarmAppendInfo. (Note do not
				                confuse this with a change of additional text.)
				
				                A management system should be prepared for
				                appending text to additional info, indicated by
				                the alarmNObjMoreAdditionalInfo varbind, and
				                sent with alarmAppendInfo.  (Note do not
				                confuse this with a change of additional info.)
				
				                A management system should also be prepared to
				                receive a resource ID (OID) identifying the
				                alarming resource if the system sending the
				                notification can provide that information. In
				                that case, alarmNObjResourceId will be set to
				                'true' and the resource ID will be sent in an
				                alarmAppendInfo notification."
			::= { rauNotifPrefix 2 }

		
		-- 1.3.6.1.4.1.193.235.1.1.2.1.2.1.3
		alarmMinor NOTIFICATION-TYPE
			OBJECTS { alarmActiveManagedObject, alarmActiveMajorType, alarmActiveMinorType, alarmActiveSpecificProblem, alarmActiveLastSequenceNo, 
				alarmActiveEventType, alarmActiveEventTime, alarmActiveProbableCause, alarmActiveAdditionalText, alarmActiveAdditionalInfo, 
				alarmNObjRecordType }
